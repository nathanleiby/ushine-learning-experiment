  function pad(num, size) {
      var s = num+"";
      while (s.length < size) s = "0" + s;
      return s;
  }

  var count=15*60;
  var counter;
  function timer()
  {
    // Tick
    count=count-1;

    // Update the clock
    var minutesRemaining = pad (Math.floor( count / 60 ), 2);
    var secondsRemaining = pad (count % 60 , 2);
    $('#header-timer').text(minutesRemaining + ':' + secondsRemaining + " remaining");

    // Check if time expired
    if (count <= 0)
    {
      clearInterval(counter);
      guiders.show('end1');
    }
  }

  function continueExperiment() {
    guiders.hideAll();
    count=5*60;
    counter=setInterval(timer, 1000); // tick every 1 second
  }

  function quitExperiment() {
    guiders.hideAll()
    clearInterval(counter);
    socket.emit('end_experiment', 'out of time');
    lastMessageEndExperiment = true;
    guider.show('end2');
  }

  // Fix tokenization to allow easy highlighting but keep commas, symbols, and line breaks
  function tokenizeText(text) {
    var tokens = text.split(" ");
    var updated_tokens = [];

    // Split on ,
    for (var t in tokens) {
      console.log(tokens[t]);
      var split_token = tokens[t].split(',');
      console.log(split_token);
      for (var st in split_token) {
        if (st != 0) {
          updated_tokens.push(',');  
        }
        updated_tokens.push(split_token[st]);
      }
    }
    tokens = updated_tokens;
    updated_tokens = [];

    // Split on \n
    for (var t in tokens) {
      console.log(tokens[t]);
      var split_token = tokens[t].split('\n');
      console.log(split_token);
      for (var st in split_token) {
        if (st != 0) {
          updated_tokens.push('<br /><br />');  
        }
        updated_tokens.push(split_token[st]);
      }
    }
    //- //- return text.split(/[\s,]+/);
    tokens = updated_tokens;
    return tokens;
  }

