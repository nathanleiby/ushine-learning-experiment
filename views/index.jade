  function updateExperimentStage(active_stage) {
    // have all hidden by default in layout.jade file
    console.log(active_stage);
    for (var s in experimentStages) {
      var stage_id = experimentStages[s];
      $('#' + stage_id).prop('class', 'inactiveStage');
      $('#' + stage_id).hide();
      if (active_stage == stage_id) {
        $('#' + stage_id).show();
        $('#' + stage_id).prop('class', 'activeStage');
      }
    }
  }

