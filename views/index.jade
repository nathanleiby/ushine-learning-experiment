  function getExitSurvey() {
    var taskNames = ['Pii', 'Location', 'Url', 'Category'];
    var reviewNames = ['Tedious', 'Difficult', 'Experience', 'Precision'];
    
    var results = {};

    for (var r in reviewNames) {
      for (var t in taskNames) {
        var itemName = 'exit' + reviewNames[r] + taskNames[t];
        results[itemName] = _getRadioValue('#' + itemName);
      }
    }
    
    results['exitUI'] = $('textarea#exitUIInput').prop('value');
    results['exitComments'] = $('textarea#exitCommentsInput').prop('value');
    return results;
  }

  function _getRadioValue(css_selector) {
    //- console.log(css_selector);
    var checkedItem = $(css_selector + ' input[type=radio]:checked')[0];
    if (checkedItem) {
      return checkedItem.value;
    } else {
      return "";
    }
  }
  
