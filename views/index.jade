  function nextTask() {
    console.log('next task');

    currentTaskIdx++;
    currentTaskIdx %= taskDivs.length;

    // Show tutorial if mesageIdx == 0
    if (SHOW_TUTORIAL) {
      if (messageIdx == 0) {
        _getTutorialGuider();
      }
    }

    taskStartTimes[taskDivs[currentTaskIdx]] = new Date().getTime();

    //- console.log('current task idx =', currentTaskIdx)
    for (t in taskDivs) {
      var css_selector = "div#" + taskDivs[t];

      if (t == currentTaskIdx) {
        // Enable current task
        $(css_selector).addClass('enabled');
        $(css_selector).children().prop('disabled',false);   
      } else {
        // Disable other tasks
        $(css_selector).removeClass('enabled')
        $(css_selector).children().prop('disabled',true);   
      }

      var css_selector = "div#category input:checkbox";
      if (taskDivs[currentTaskIdx] == 'category') {
        // enable checkboxes
        $(css_selector).prop('disabled',false); 
      } else {
        // disable checkboxes
        $(css_selector).prop('disabled',true); 
      }

    }

    // Emit start_task event
    socket.emit('start_task', messageIdx, currentMessage.id, currentTaskIdx, taskDivs[currentTaskIdx]);
  }

