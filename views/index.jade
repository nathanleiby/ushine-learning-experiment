  function clickMessageToken(e) {
    if (taskDivs[currentTaskIdx] != 'pii') {
      return;
    }

    var my_id = $(this).attr('id');
    token_text = $('#' + my_id).text()
    console.log('clicked token', my_id);
    var token = {
      'idx' : my_id.replace(TOKEN_INDEX_TAG, ''),
      'text' : token_text,
    };

    var tokenExistsAlready = false;
    for (var i=0; i<selections.length; i++) {
      if (selections[i].idx == token.idx && selections[i].text == token.text) {
        tokenExistsAlready = true;
        selections.splice(i,1);
        break;
      }   
    }

    if (! tokenExistsAlready) {
      selections.push(token);
    }
    console.log(selections);

    // Update all highlighting
    updateMessageHighlighting();
  }

  function _suggestionHighlightHelper(highlight_type, token, t) {
    //  Which suggestions?
    var suggestions = [];
    if (highlight_type == 'pii' && experimentMode == 1) {
      suggestions = currentMessage.suggested_pii;
    } else if (highlight_type == 'pii' && experimentMode == 2) {
      suggestions = currentMessage.actual_pii;
    } else if (highlight_type == 'location' && experimentMode == 1) {
      suggestions = currentMessage.suggested_location_name;
    } else if (highlight_type == 'location' && experimentMode == 2) {
      suggestions = currentMessage.actual_location_name;
    } else if (highlight_type == 'url' && experimentMode == 1) {
      suggestions = currentMessage.suggested_url;
    } else if (highlight_type == 'url' && experimentMode == 2) {
      suggestions = currentMessage.actual_url;
    } else {
      return;
    }

    for (var i in suggestions) {
      var pii = suggestions[i];
      if (token.indexOf(pii) > -1) {
        var css_selector = '#' + TOKEN_INDEX_TAG + t;
        $('#' + TOKEN_INDEX_TAG + t).addClass('guess-' + highlight_type);
      } 
    }
  }

  function updateMessageSuggestionHighlighting() {
    console.log('updateMessageSuggestionHighlighting');

    // Find token indexes
    for (var t in message_tokens) {
      // Check if PII is present
      var token = message_tokens[t];
      _suggestionHighlightHelper("pii", token, t);
      _suggestionHighlightHelper("location", token, t);
      _suggestionHighlightHelper("url", token, t);
    }
  }

  function updateMessageHighlighting() {
    console.log('updateMessageHighlighting');

    // PII only
    for (var t in message_tokens) {
      $('#' + TOKEN_INDEX_TAG + t).removeClass('selected-pii');
    }

    for (var s in selections) {
      var sel = selections[s];
      var t = sel.idx;
      $('#' + TOKEN_INDEX_TAG + t).addClass('selected-pii');
    }
  }
  
